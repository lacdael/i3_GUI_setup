! Dracula Xresources palette

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window: #44475a, #f1fa8c, #f1fa8c

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal: #44475a, #8BE9FD, #44475a, #44475a, #FF7AC6
rofi.color-active: #f1fa8c, #44475a, #8BE9FD, #44475a, #FF7AC6
rofi.color-urgent: #f1fa8c, #44475a, #8BE9FD, #44475a, #FF7AC6
r
! Set the desired separator style
rofi.separator-style: solid

